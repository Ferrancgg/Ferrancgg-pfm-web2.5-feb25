{
  "compilerOptions": {
    "declaration": true,  
    "emitDeclarationOnly": false,  
    "outDir": "./dist",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "target": "ES6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
